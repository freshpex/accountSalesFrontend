{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://accountsalesb.vercel.app/api/:path*"
    },
    {
      "source": "/user/auth/google",
      "destination": "https://accountsalesb.vercel.app/user/auth/google"
    },
    {
      "source": "/api/v1/user/auth/google",
      "destination": "https://accountsalesb.vercel.app/api/v1/user/auth/google"
    },
    {
      "source": "/api/v1/user/auth/google/callback",
      "destination": "https://accountsalesb.vercel.app/api/v1/user/auth/google/callback"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}